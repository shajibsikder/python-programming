<script type="text/javascript">
   $.getScript('{{CONFIG theme_url}}/js/Sortable.min.js', function(data, textStatus) {
      var el = document.getElementById('next-video-list');
      var sortable = Sortable.create(el,{
         onEnd: function (/**Event*/evt) {
            var video_array = [];
            $('[data-litsitem-id]').each(function(i, obj) {
               video_array[i] = $(obj).attr('data-video-id');
               if (i == $('[data-litsitem-id]').length-1) {
                  $.post('{{LINK aj/lists?type=list&a=sort}}', {video_array: video_array,type:'list'}, function(data, textStatus, xhr) {});
               }
            });
         }
      });
   });
</script>
<?php } ?>
